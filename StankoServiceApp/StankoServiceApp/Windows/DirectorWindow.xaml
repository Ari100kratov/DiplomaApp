<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
       xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" x:Class="StankoServiceApp.Windows.DirectorWindow"
       dx:ThemeManager.ThemeName="none"
        mc:Ignorable="d"
        Title="" Height="450" Width="800" Icon="/StankoServiceApp;component/Resources/icon.png" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal">
            <Button x:Name="sbWorker" Content="Сотрудники" Width="auto" HorizontalAlignment="Left" FontSize="18" Margin="10 10 0 10" Click="sbWorker_Click"/>
            <Button x:Name="sbProject" Content="Проекты" Width="auto" HorizontalAlignment="Left" FontSize="18" Margin="0 10 0 10" Click="sbProject_Click"/>
            <Button x:Name="sbTask" Content="Задачи" Width="auto" HorizontalAlignment="Left" FontSize="18" Margin="0 10 0 10" Click="sbTask_Click"/>
        </StackPanel>
        <Frame x:Name="MainFrame" Grid.Row="1"/>
    </Grid>
</Window>
